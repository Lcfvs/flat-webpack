{
  "css": {
    "bundles": {
      "app": {
        "main": "./assets/css/dev/main.css",
        "style": "./assets/css/dev/style.css"
      }
    },
    "config": {
      "module": {
        "loaders": [
          {
            "loader": "file-loader",
            "options": {
              "name": "../dist/[name].min.css"
            }
          },
          {
            "loader": "extract-loader"
          },
          {
            "loader": "css-loader",
            "options": {
              "minimize": true
            }
          }
        ]
      },
      "output": {
        "path": "./assets/css/build",
        "filename": "[name].min.js"
      },
      "plugins": [
        {
          "name": "webpack-css-concat-plugin",
          "params": [
            {
              "filename": "../dist/app.min.css"
            }
          ]
        },
        {
          "name": "webpack/lib/BannerPlugin"
        }
      ],
      "target": "web"
    }
  },
  "js": {
    "bundles": {
      "app": [
        {
          "src": "js-polyfills/url.js",
          "alias": "URL"
        },
        {
          "src": "es6-promise/dist/es6-promise.auto.js",
          "alias": "Promise"
        },
        {
          "src": "whatwg-fetch",
          "alias": "fetch"
        },
        {
          "src": "anticore"
        },
        {
          "src": "./assets/js/dev/main.js"
        }
      ]
    },
    "config": {
      "devtool": "source-map",
      "module": {
        "loaders": [
          {
            "loader": "babel-loader",
            "options": {
              "presets": [
                "env"
              ]
            }
          }
        ]
      },
      "output": {
        "path": "./assets/js/dist"
      },
      "plugins": [
        {
          "name": "babel-minify-webpack-plugin"
        },
        {
          "name": "webpack/lib/BannerPlugin"
        }
      ],
      "target": "web"
    }
  }
}